{
  "name": "resume-online",
  "version": "1.0.0",
  "description": "Social network to create and share CV",
  "main": "server.js",
  "engine": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
